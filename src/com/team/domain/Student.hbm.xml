<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.team.domain.Student" table="student" schema="" catalog="stu">
        <id name="stuId" column="stu_id"/>
        <property name="stuName" column="stu_name"/>
        <property name="stuSex" column="stu_sex"/>
        <property name="stuGrade" column="stu_grade"/>
        <property name="schId" column="sch_id"/>
    </class>
</hibernate-mapping>